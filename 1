public class Main
{
  public static void main (String[]args)
  {

    int count = 0;
    int arr[] = { 90, 50, 40, 100, 20, 4, 10, 25, 1, 100 };
    for (int i = 0; i <= arr.length; i++)
      {
	for (int j = i + 1; j < arr.length; j++)
	  {
	    if (arr[i] > 2 * arr[j])
	      {
		count++;
	      }
	  }
      }
    System.out.println ("The Value is " + count);
  }
}
